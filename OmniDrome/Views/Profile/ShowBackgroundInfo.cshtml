<div>
    <button type="button" id="btn_viewtl" class="btn btn-primary">View TimeLine</button>
</div>
<script>
    $('#btn_viewtl').click(function () {
        $('#main_timeline').css("visibility", "visible");
        $('<div id="overlay">').css({
            width: $(document).width(),
            height: $(document).height(),
            opacity: 0.6,
            position: 'fixed',
            top: 0,
            left: 0,
            backgroundColor: 'black',
            zIndex: 40
        }).appendTo('body').click(function () {
            $(this).remove();
            $('#main_timeline').css("visibility", "hidden");
        });
    });
</script>
        

